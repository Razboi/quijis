{
  "name": "Quijis - Quick JIRA issues",
  "version": "0.5.3",
  "description": "Create JIRA issues with automatic error logs and video attachment.",
  "permissions": [
    "declarativeContent",
    "storage",
    "debugger",
    "desktopCapture",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "src/background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "src/views/record/record.html",
    "default_icon": {
      "16": "src/assets/images/quijis_16.png",
      "32": "src/assets/images/quijis_32.png",
      "48": "src/assets/images/quijis_48.png",
      "128": "src/assets/images/quijis_128.png"
    }
  },
  "icons": {
    "16": "src/assets/images/quijis_16.png",
    "32": "src/assets/images/quijis_32.png",
    "48": "src/assets/images/quijis_48.png",
    "128": "src/assets/images/quijis_128.png"
  },
  "manifest_version": 2
}